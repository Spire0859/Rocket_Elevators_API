                  <input required type="email" value="" class="form-control" name="leads[email]" id="email">
               </div>
               <div class="col-md-4">
                  <label for="contact:phone">Phone</label>
                  <input type="text" value="" class="form-control" name="leads[phoneNumber]" id="phone">
               </div>
            </div>
            <div class="row">
               <div class="col-md-4">
                  <label for="contact:name">Buisiness Name</label>
                  <input required type="text" value="" class="form-control" name="leads[companyName]" id="company_name">
               </div>
               <div class="col-md-4">
                  <label for="contact:email">Project Name</label>
                  <input required type="text" value="" class="form-control" name="leads[nameProject]" id="project_name">
               </div>
               <div class="col-md-4">
                  <label for="contact_department">Department</label>
                  <select class="form-control pointer" id="department-type" onchange="department()">
                     <option value="waw" selected="true" disabled >--- Select ---</option>
                     <option value="Marketing">Residential</option>
                     <option value="Webdesign">Commercial</option>
                  </select>
               </div>
            </div>
            <div class="row">
               <div class="col-md-12">
                  <label for="contact:message">Project Description</label>
                  <textarea required maxlength="100" rows="4" class="form-control" name="leads[descriptionProject]" id="project_description"></textarea>
               </div>
            </div>
            <div class="row">
               <div class="col-md-12">
                  <label for="contact:message">Message *</label>
                  <textarea required maxlength="10000" rows="8" class="form-control" name="leads[message]" id="message"></textarea>
               </div>
            </div>
            <div class="row">
               <div class="col-md-12">
